<h1>Table of All Songs</h1>
<table>
    <table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Year</th>
      <th>Genre</th>
      <th>Artist name</th>
      <th></th>
    </tr>
  </thead>
     <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.release_year %></td>
        <td><%= song.genre %></td>
        <td><%= song.artist_name %></td>
        <%= link_to 'Create New Song', new_song_path %>
         <%= link_to 'Edit Song', edit_song_path(song) %>
           <%= link_to 'Delete Song', song_path, method: :delete %>
            <%= link_to 'View Song', song_path(song) %>
      </tr>
    <% end %>
  </tbody>
</table>
